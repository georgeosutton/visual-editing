{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "env": ["ANALYZE", "APP_ENV"],
        "outputs": ["dist/**", ".next/**"]
      },
      "lint:js": {
        "inputs": ["**/*.js", "**/*.jsx", "**/*.mjs", "**/*.ts", "**/*.tsx"]
      },
      "lint:types": {
        "inputs": ["**/*.ts", "**/*.tsx"]
      },
      "dev": {
        "cache": false
      }
    }
  }